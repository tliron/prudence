#!/bin/bash
set -e

HERE=$(dirname "$(readlink --canonicalize "$BASH_SOURCE")")
. "$HERE/_env"

"$HERE/build"

if [ "$PROTOCOL" == https ]; then
    prudence run "$ROOT/examples/hello-world/start.js" --argument=hello=world --argument=secure=true -vv
else
    prudence run "$ROOT/examples/hello-world/start.js" --argument=hello=world -vv
fi
